	<!-- for autocomplete List  -->
	<script>
		var x, autoDataList
		window.onload = function () {
			x = document.getElementById("myInput").value
			autoDataList = JSON.parse(x)

			document.getElementById('search').addEventListener('input', (e) => {
				let fruitsArray = [];
				if (e.target.value) {
					fruitsArray = autoDataList.filter(fruit => fruit.toLowerCase().startsWith(e.target.value));
				}
				showFruitsArray(fruitsArray);
			})
		};
		function showFruitsArray(fruitsArray) {
			const html = !fruitsArray.length ? "No results found" : fruitsArray.join('\n');
			document.querySelector('ul').innerHTML = html
		}
	</script>